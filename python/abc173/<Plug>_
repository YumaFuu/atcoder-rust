h, w, k = map(int, input().split())

c = [0] * h
for i in range(h):
    c[i] = input()

for cols in range(1 << w):
    for rows in range(1 << h):
        # print("{}: {}".format(format(cols, "b"), format(rows, "b")))
        for i in range(cols):
            print(cols)
            print(cols >> i)

